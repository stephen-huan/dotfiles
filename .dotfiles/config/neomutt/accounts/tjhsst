set from      = "2021shuan@tjhsst.edu"
set sendmail  = "$my_sendmail_program -a tjhsst"
set mbox      = "+tjhsst/archive"
# unlike gmail, webmail doesn't automatically copy to sent folder
set record    = "+tjhsst/sent"
set postponed = "+tjhsst/drafts"
# remove from inbox (since a copy is stored in all mail)
bind index,pager I purge-message
set trash     = "+tjhsst/trash"

