snippet pyproject pyproject.toml defaults
	[tool.pyright]
	exclude = [".venv"]
	venvPath = "."
	venv = ".venv"
	
	[tool.ruff]
	line-length = 79
	
	[tool.black]
	line-length = 79
	
	[tool.isort]
	profile = "black"
	line_length = 79

snippet juliafmt .JuliaFormatter.toml defaults
	style = "blue"
	margin = 79
